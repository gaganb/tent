{{view Tent.Button class="launch" labelBinding="view.label" aria-haspopup="true" action="launch" targetBinding="view" typeBinding="view.type"}}

<div class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-header">
	  <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
	  {{view Tent.ModalHeader}}
	</div>
	<div class="modal-body">
	  {{#if view.text}}
	  	{{view Tent.ModalBody}}
	  {{/if}}
	  {{yield}}
	</div>
	<div class="modal-footer">
	  {{#if view.secondary}}<a href="#" class="btn btn-secondary" data-dismiss="modal" click="secondary">{{loc view.secondary}}</a>{{/if}}
	  {{#if view.primary}}<a href="#" class="btn btn-primary" data-dismiss="modal" click="primary">{{loc view.primary}}</a>{{/if}}
	</div>
</div>